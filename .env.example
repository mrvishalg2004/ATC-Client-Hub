# Copy to .env.local and set to your MongoDB Atlas connection string
MONGODB_URI=mongodb+srv://<username>:<password>@cluster.mongodb.net/campusconnect?retryWrites=true&w=majority
MONGODB_DB_NAME=campusconnect

# Optional: email automation via Resend
RESEND_API_KEY=
NOTIFICATION_FROM_EMAIL=notifications@example.com
NOTIFICATION_EMAIL=owner@example.com
